	Note2 [label="Practice with simple programs and projects to reinforce understanding." fillcolor="#B9FBC0" shape=note]
	Note3 [label="Working on projects helps solidify knowledge and builds problem-solving skills." fillcolor="#B9FBC0" shape=note]
	LearningObjectives -> LearningPath
	LearningPath -> PracticeProjects
	LearningObjectives -> Note1 [style=dashed]
	LearningPath -> Note2 [style=dashed]
	PracticeProjects -> Note3 [style=dashed]
	subgraph cluster_learning {
		fillcolor="#FFD6A5" style=filled
		LearningObjectives
		Note1
		fontsize=12 label="Learning Objectives" labeljust=l labelloc=t
	}
	subgraph cluster_path {
